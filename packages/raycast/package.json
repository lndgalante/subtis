{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "subtis",
  "title": "Subtis",
  "description": "The subtitle you were looking for",
  "author": "leonardo_galante",
  "license": "MIT",
  "icon": "command-icon.png",
  "version": "0.4.7",
  "commands": [
    {
      "name": "index",
      "title": "Find Subtitle",
      "description": "Find the subtitle you were looking for",
      "mode": "view"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "deploy": "rm -rf .env.development && bun run build && npx @raycast/api@latest publish"
  },
  "dependencies": {
    "@raycast/api": "^1.88.4",
    "node-fetch": "^3.3.2",
    "ts-pattern": "^5.6.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.11",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "19.0.2",
    "eslint": "9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
