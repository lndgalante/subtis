{
  "name": "@subtis/db",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "dev": "bun --watch app.ts",
    "test:watch": "bun test --watch",
    "supabase:type:definitions": "bunx supabase gen types typescript --project-id 'yelhsmnvfyyjuamxbobs' --schema public > types/types.ts",
    "supabase:schemas": "supabase-to-zod --input types/types.ts --output schemas/schemas.ts",
    "generate": "bun run supabase:type:definitions && bun run supabase:schemas"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bun": "^1.0.10",
    "supabase": ">=1.150.0",
    "supabase-to-zod": "^1.0.5",
    "typescript": "^5.4.3"
  },
  "trustedDependencies": [
    "supabase"
  ]
}
