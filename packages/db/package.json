{
  "name": "db",
  "version": "0.0.1",
  "license": "MIT",
  "main": "app.ts",
  "scripts": {
    "dev": "bun --watch app.ts",
    "test:watch": "bun test --watch",
    "supabase:type:definitions": "bunx supabase gen types typescript --project-id 'yelhsmnvfyyjuamxbobs' --schema public > types.ts",
    "supabase:schemas": "supabase-to-zod --input types.ts --output schemas.ts",
    "generate": "bun run supabase:type:definitions && bun run supabase:schemas"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "bun-types": "^1.0.19",
    "supabase": ">=1.123.4",
    "supabase-to-zod": "^1.0.4",
    "typescript": "^5.3.3"
  },
  "trustedDependencies": [
    "supabase"
  ]
}
